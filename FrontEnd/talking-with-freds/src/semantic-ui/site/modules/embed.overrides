/*******************************
         Site Overrides
*******************************/
.ui.embed {
    &[customsize] {
        width: 930px;
        margin: -5rem auto 5rem;
        height: 520px;
        padding-bottom: 0;
        box-shadow: 5px 0px 25px 3px #9794946b;
    }
    &[customsizemobile] {
        width: 80%;
        min-height: 200px;
        margin: -150px auto 70px;
        height: 20rem;
        padding-bottom: 0;
    }
}